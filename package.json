{
  "name": "londonfox-react-client-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for React client projects'",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch --progress && npm run start",
    "build": "webpack --mode development",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/londonfox/londonfox-react-client-boilerplate"
  },
  "author": "Charles Markovich",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-node-externals": "^1.7.2"
  }
}
